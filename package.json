{
  "name": "api-codegen-universal",
  "version": "0.1.0",
  "description": "通用 API 代码生成器 - 支持多种 API 规范的统一 TypeScript 接口生成",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "codegen",
    "openapi",
    "swagger",
    "typescript",
    "api",
    "generator",
    "interface",
    "type"
  ],
  "author": "XSIJIE975",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/XSIJIE975/api-codegen-universal.git"
  },
  "bugs": {
    "url": "https://github.com/XSIJIE975/api-codegen-universal/issues"
  },
  "homepage": "https://github.com/XSIJIE975/api-codegen-universal#readme",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm -r --filter=\"./packages/**\" build && rslib build",
    "build:packages": "pnpm -r --filter=\"./packages/**\" build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint \"{src,packages,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,packages,test}/**/*.ts\" --fix",
    "test": "rstest",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build && pnpm run test",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@redocly/openapi-core": "^2.11.1",
    "js-yaml": "^4.1.1",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.0",
    "@microsoft/api-extractor": "^7.55.0",
    "@rsbuild/plugin-source-build": "^1.0.3",
    "@rslib/core": "^0.17.2",
    "@rstest/core": "^0.6.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.2"
  }
}
