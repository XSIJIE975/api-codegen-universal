{
  "name": "api-codegen-universal",
  "version": "0.6.2",
  "description": "Universal API code generation adapter for OpenAPI (Swagger) and Apifox, producing standardized TypeScript types and API metadata.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "codegen",
    "openapi",
    "swagger",
    "typescript",
    "api",
    "generator",
    "interface",
    "type"
  ],
  "author": "XSIJIE975",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/XSIJIE975/api-codegen-universal.git",
    "directory": "packages/api-codegen-universal"
  },
  "homepage": "https://github.com/XSIJIE975/api-codegen-universal#readme",
  "bugs": {
    "url": "https://github.com/XSIJIE975/api-codegen-universal/issues"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@redocly/openapi-core": "^2.11.1",
    "js-yaml": "^4.1.1",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@api-codegen-universal/apifox": "workspace:*",
    "@api-codegen-universal/core": "workspace:*",
    "@api-codegen-universal/openapi": "workspace:*",
    "@microsoft/api-extractor": "^7.55.0",
    "@rsbuild/plugin-source-build": "^1.0.3",
    "@rslib/core": "^0.17.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.0"
  }
}
