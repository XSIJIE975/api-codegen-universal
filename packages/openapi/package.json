{
  "name": "@api-codegen-universal/openapi",
  "version": "0.1.0",
  "private": true,
  "description": "OpenAPI adapter for api-codegen-universal",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch"
  },
  "dependencies": {
    "@api-codegen-universal/core": "workspace:*",
    "typescript": "^5.9.3",
    "@redocly/openapi-core": "^2.11.1",
    "js-yaml": "^4.1.1",
    "openapi-typescript": "^7.10.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@rslib/core": "^0.17.2"
  }
}
